{
  "name": "auto-google-drive-access-request-monthly-payment",
  "displayName": "01 Auto Google Drive Access Request Tool",
  "version": "0.0.1",
  "description": "The second you open a restricted Google Doc, it fires off the access request automatically.",
  "author": "Marco Tsang",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:plasmo": "plasmo dev",
    "dev:next": "next dev --port 8472",
    "build": "run-p build:*",
    "build:plasmo": "plasmo build",
    "build:next": "next build"
  },
  "dependencies": {
    "@plasmohq/storage": "^1.15.0",
    "google-auth-library": "9.4.2",
    "next": "14.1.0",
    "plasmo": "0.90.5",
    "puro": "0.8.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "stripe": "14.13.0",
    "swr": "2.2.4"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@plasmohq/rps": "1.8.7",
    "@types/chrome": "0.0.258",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "prettier": "3.2.4",
    "tailwindcss": "^3.4.17",
    "typescript": "5.3.3"
  },
  "manifest": {
    "key": "$CRX_PUBLIC_KEY",
    "oauth2": {
      "client_id": "$OAUTH_CLIENT_ID",
      "scopes": [
        "https://www.googleapis.com/auth/userinfo.email",
        "https://www.googleapis.com/auth/userinfo.profile"
      ]
    },
    "host_permissions": [
      "$PLASMO_PUBLIC_API_URI/*",
      "https://*/*"
    ],
    "permissions": [
      "identity",
      "identity.email"
    ]
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "svgo": "2.8.0"
      }
    }
  }
}
